root=true

[*]
charset = utf-8
end_of_line = lf
tab_width = 4
trim_trailing_whitespace = true
insert_final_newline = true

[brachyurabootstrapconf.txt]
indent_style = tab

[*.java]
indent_style = tab
ij_continuation_indent_size = 4
ij_java_class_count_to_use_import_on_demand = 99999
ij_java_names_count_to_use_import_on_demand = 99999

[*.json]
indent_style = space
indent_size = 2

[.editorconfig]
indent_style = space
indent_size = 4
